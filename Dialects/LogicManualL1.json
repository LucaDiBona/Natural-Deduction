
{
   "Connectives" : {
       "NOT" : {
           "Inputs"    : ["!"],
           "Display" : "¬",
           "LaTeX"   : "\\lnot"
       },
       "AND" : {
           "Inputs"    : ["&"],
           "Display" : "∧",
           "LaTeX"   : "\\wedge"
       },
       "OR"  : {
           "Inputs"    : ["|"],
           "Display" : "∨",
           "LaTeX"   : "\\vee"
       },
       "IF"  : {
           "Inputs"    : [">"],
           "Display" : "→",
           "LaTeX"   : "\\to"
       },
       "IFF" : {
           "Inputs"    : ["-"],
           "Display" : "⟷",
           "LaTeX"   : "\\leftrightarrow"
       }
   },
   "Letters" : {
       "SINGLE LETTER" : {
           "Inputs"    : "([PQR])",
           "Display"   : "\\1",
           "LaTeX"     : "\\1"
       },
       "NUMBERED LETTER":{
           "Inputs"    : "([PQR])([1-9][0-9]*)",
           "Display"   : "\\1\\2",
           "LaTeX"     : "\\1_{\\2}"
}
   }
    "Rules" : {}
}