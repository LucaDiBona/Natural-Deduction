{
  "Name": "L1 - Logic Manual",
  "Connectives": {
    "NOT": {
      "Inputs": ["!"],
      "Display": "¬",
      "LaTeX": "\\lnot"
    },
    "AND": {
      "Inputs": ["&"],
      "Display": "∧",
      "LaTeX": "\\wedge"
    },
    "OR": {
      "Inputs": ["|"],
      "Display": "∨",
      "LaTeX": "\\vee"
    },
    "THEN": {
      "Inputs": [">"],
      "Display": "→",
      "LaTeX": "\\to"
    },
    "IFF": {
      "Inputs": ["-"],
      "Display": "⟷",
      "LaTeX": "\\leftrightarrow"
    }
  },
  "Letters": {
    "SINGLE LETTER": {
      "Input": "([PQR])",
      "Display": "\\1",
      "LaTeX": "\\1"
    },
    "NUMBERED LETTER": {
      "Input": "([PQR])([1-9][0-9]*)",
      "Display": "\\1\\2",
      "LaTeX": "\\1_{\\2}"
    },
    "PHI": {
      "Input": "F|f|PHI|Phi|phi",
      "Display": "ɸ",
      "LaTeX": "\\phi"
    },
    "PSI": {
      "Input": "S|s|PSI|Psi|psi",
      "Display": "ψ",
      "LaTeX": "\\psi"
    },
    "CHI": {
      "Input": "C|c|X|x|CHI|Chi|chi",
      "Display": "χ",
      "LaTeX": "\\chi"
    }
  },
  "Rules": {
    "AND INTRO": {
      "Display": "∧Intro",
      "LaTeX": "$\\wedge$Intro",
      "Inputs": ["$P", "$Q"],
      "Output": "$P AND $Q"
    },
    "AND ELIM 1": {
      "Display": "∧Elim1",
      "LaTeX": "$\\wedge$Elim1",
      "Inputs": ["$P AND $Q"],
      "Output": "$P"
    },
    "AND ELIM 2": {
      "Display": "∧Elim2",
      "LaTeX": "$\\wedge$Elim2",
      "Inputs": ["$P AND $Q"],
      "Output": "$Q"
    },
    "THEN INTRO": {
      "Display": "→Intro",
      "LaTeX": "$\\to$Intro",
      "Inputs": [["$P", "$Q"]],
      "Output": "$P THEN $Q"
    },
    "THEN ELIM": {
      "Display": "→Elim",
      "LaTeX": "$\\to$Elim",
      "Inputs": ["$P", "$P THEN $Q"],
      "Output": "$Q"
    },
    "OR INTRO 1": {
      "Display": "∨Intro1",
      "LaTeX": "$\\vee$Intro1",
      "Inputs": ["$P"],
      "Output": "$P OR $Q"
    },
    "OR INTRO 2": {
      "Display": "∨Intro2",
      "LaTeX": "$\\vee$Intro2",
      "Inputs": ["$Q"],
      "Output": "$P OR $Q"
    },
    "OR ELIM": {
      "Display": "∨Elim",
      "LaTeX": "$\\vee$Elim",
      "Inputs": ["$P OR $Q", ["$P", "$R"], ["$Q", "$R"]],
      "Output": "$R"
    },
    "NOT INTRO": {
      "Display": "¬Intro",
      "LaTeX": "$\\lnot$Intro",
      "Inputs": [
        ["$P", "$Q"],
        ["$P", "NOT $Q"]
      ],
      "Output": "NOT $P"
    },
    "NOT ELIM": {
      "Display": "¬Elim",
      "LaTeX": "$\\lnot$Elim",
      "Inputs": [
        ["NOT $P", "$Q"],
        ["NOT $P", "NOT $Q"]
      ],
      "Output": "$P"
    },
    "IFF INTRO": {
      "Display": "⟷Intro",
      "LaTeX": "$\\leftrightarrow$Intro",
      "Inputs": ["$P THEN $Q", "$Q THEN $P"],
      "Output": "$P IFF $Q"
    },
    "IFF ELIM 1": {
      "Display": "⟷Elim1",
      "LaTeX": "$\\leftrightarrow$Elim1",
      "Output": "$P IFF $Q"
    },
    "IFF ELIM 1": {
      "Display": "⟷Elim1",
      "LaTeX": "$\\leftrightarrow$Elim1",
      "Inputs": ["$P IFF $Q"],
      "Output": "$P THEN $Q"
    },
    "IFF ELIM 2": {
      "Display": "⟷Elim2",
      "LaTeX": "$\\leftrightarrow$Elim2",
      "Inputs": ["$P IFF $Q"],
      "Output": "$Q THEN $P"
    }
  }
}
